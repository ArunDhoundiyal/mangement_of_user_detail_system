#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/user-management/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/bin/node_modules:/home/theia/user-management/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/node_modules:/home/theia/user-management/node_modules/.pnpm/typescript@4.9.5/node_modules:/home/theia/user-management/node_modules/.pnpm/node_modules:/home/theia/user-management/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/user-management/node_modules/.pnpm/ts-node@10.9.1_410642ec697b4b71bc2c2c797f2f615c/node_modules/ts-node/node_modules:/home/theia/user-management/node_modules/.pnpm/ts-node@10.9.1_410642ec697b4b71bc2c2c797f2f615c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/user-management/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/bin/node_modules:/home/theia/user-management/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/node_modules:/home/theia/user-management/node_modules/.pnpm/typescript@4.9.5/node_modules:/home/theia/user-management/node_modules/.pnpm/node_modules:/home/theia/user-management/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/user-management/node_modules/.pnpm/ts-node@10.9.1_410642ec697b4b71bc2c2c797f2f615c/node_modules/ts-node/node_modules:/home/theia/user-management/node_modules/.pnpm/ts-node@10.9.1_410642ec697b4b71bc2c2c797f2f615c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.9.5/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.9.5/node_modules/typescript/bin/tsserver" "$@"
fi
