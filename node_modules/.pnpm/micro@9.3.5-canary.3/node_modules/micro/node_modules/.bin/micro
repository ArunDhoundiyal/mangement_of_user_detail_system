#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/user-management/node_modules/.pnpm/micro@9.3.5-canary.3/node_modules/micro/bin/node_modules:/home/theia/user-management/node_modules/.pnpm/micro@9.3.5-canary.3/node_modules/micro/node_modules:/home/theia/user-management/node_modules/.pnpm/micro@9.3.5-canary.3/node_modules:/home/theia/user-management/node_modules/.pnpm/node_modules:/home/theia/user-management/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/user-management/node_modules/.pnpm/micro@9.3.5-canary.3/node_modules/micro/bin/node_modules:/home/theia/user-management/node_modules/.pnpm/micro@9.3.5-canary.3/node_modules/micro/node_modules:/home/theia/user-management/node_modules/.pnpm/micro@9.3.5-canary.3/node_modules:/home/theia/user-management/node_modules/.pnpm/node_modules:/home/theia/user-management/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/micro.js" "$@"
else
  exec node  "$basedir/../../bin/micro.js" "$@"
fi
