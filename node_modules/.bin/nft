#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/user-management/node_modules/.pnpm/@vercel+nft@0.27.0/node_modules/@vercel/nft/out/node_modules:/home/theia/user-management/node_modules/.pnpm/@vercel+nft@0.27.0/node_modules/@vercel/nft/node_modules:/home/theia/user-management/node_modules/.pnpm/@vercel+nft@0.27.0/node_modules/@vercel/node_modules:/home/theia/user-management/node_modules/.pnpm/@vercel+nft@0.27.0/node_modules:/home/theia/user-management/node_modules/.pnpm/node_modules:/home/theia/user-management/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/user-management/node_modules/@vercel/nft/out/node_modules:/home/theia/user-management/node_modules/@vercel/nft/node_modules:/home/theia/user-management/node_modules/@vercel/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/user-management/node_modules/.pnpm/@vercel+nft@0.27.0/node_modules/@vercel/nft/out/node_modules:/home/theia/user-management/node_modules/.pnpm/@vercel+nft@0.27.0/node_modules/@vercel/nft/node_modules:/home/theia/user-management/node_modules/.pnpm/@vercel+nft@0.27.0/node_modules/@vercel/node_modules:/home/theia/user-management/node_modules/.pnpm/@vercel+nft@0.27.0/node_modules:/home/theia/user-management/node_modules/.pnpm/node_modules:/home/theia/user-management/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/user-management/node_modules/@vercel/nft/out/node_modules:/home/theia/user-management/node_modules/@vercel/nft/node_modules:/home/theia/user-management/node_modules/@vercel/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vercel/nft/out/cli.js" "$@"
else
  exec node  "$basedir/../@vercel/nft/out/cli.js" "$@"
fi
